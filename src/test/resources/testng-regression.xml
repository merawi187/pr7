<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="DemoQA Regression Tests" verbose="1">
    
    <test name="Regression Tests" preserve-order="true">
        <groups>
            <run>
                <include name="regression"/>
            </run>
        </groups>
        <classes>
            <class name="com.example.tests.TextBoxTest"/>
            <class name="com.example.tests.ButtonsTest"/>
            <class name="com.example.tests.UploadDownloadTest"/>
            <class name="com.example.tests.AlertsTest"/>
            <class name="com.example.tests.BrowserWindowsTest"/>
            <class name="com.example.tests.DragAndDropTest"/>
        </classes>
    </test>
    
</suite>
